	"config": {
		"NavigationTimingSamplingFactor": false,
		"NavigationTimingFirstPaintAsiaSamplingFactor": false,
		"NavigationTimingOversampleFactor": false,
		"NavigationTimingSurveySamplingFactor": false,
		"NavigationTimingSurveyName": false
	},
	"manifest_version": 1
}
